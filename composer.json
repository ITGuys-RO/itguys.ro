{
  "require": {
    "ext-curl": "*",
    "craftcms/cms": "3.6.2",
    "doublesecretagency/craft-bootstrap": "v4.x-dev",
    "doublesecretagency/craft-cpcss": "v2.x-dev",
    "doublesecretagency/craft-cpjs": "v2.x-dev",
    "hashtagerrors/user-initials-photo": "dev-master",
    "misterbk/mix": "dev-master",
    "nystudio107/craft-pathtools": "v1.x-dev",
    "putyourlightson/craft-sendgrid": "1.2.0",
    "vaersaagod/dospaces": "dev-master",
    "vlucas/phpdotenv": "^3.4.0",
    "workingconcept/craft-cloudflare": "0.5.0"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": {
    "blasvicco.consoletoolkit": {
      "type": "vcs",
      "url": "https://github.com/blasvicco/consoletoolkit.git"
    }
  }
}
